import java.util.Scanner;
 class UnsortedArray {
public static void main(String[] args) {
int[] arr = {14,9,28,19,6,45};
int l = arr[0];
int s = arr[0];
for (int i = 1; i < arr.length; i++) {
if (arr[i] > l) {
l = arr[i];
}
if (arr[i] < s) {
s = arr[i];
}
}
System.out.println("The largest element is: " + l);
System.out.println("The smallest element is: " + s);
}
}
